{
  "name": "@melvinla/front",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "type": "module",
  "engines": {
    "node": "24.13.0"
  },
  "scripts": {
    "predev": "tsx ./scripts/prebuild.ts",
    "dev": "next dev --turbopack",
    "postdev": "tsx ./scripts/postbuild.ts",
    "prebuild": "tsx ./scripts/prebuild.ts",
    "build": "next build",
    "postbuild": "tsx ./scripts/postbuild.ts",
    "start": "next start",
    "check-types": "tsc --noemit",
    "lint": "eslint --max-warnings=0 .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "oapi-gen": "openapi-typescript ../api/internal/gen/openapi.yml -o ./src/gen/api.d.ts --root-types --root-types-no-schema-prefix",
    "knip": "knip",
    "i18n:check": "i18n-check --source en --locales ./messages --unused src --reporter table --ignore conway.quoteUrl conway.quoteBy",
    "validate-code": "pnpm run i18n:check && pnpm run knip && pnpm run check-types && pnpm run lint",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@heroui/autocomplete": "2.3.31",
    "@heroui/avatar": "2.2.24",
    "@heroui/button": "2.2.29",
    "@heroui/card": "2.2.27",
    "@heroui/chip": "2.2.24",
    "@heroui/code": "2.2.22",
    "@heroui/date-input": "2.3.29",
    "@heroui/dropdown": "2.3.29",
    "@heroui/form": "2.1.29",
    "@heroui/input": "2.4.30",
    "@heroui/link": "2.2.25",
    "@heroui/modal": "2.2.26",
    "@heroui/navbar": "2.2.27",
    "@heroui/number-input": "2.0.20",
    "@heroui/progress": "2.2.24",
    "@heroui/react": "2.8.7",
    "@heroui/select": "2.4.30",
    "@heroui/skeleton": "2.2.18",
    "@heroui/slider": "2.4.26",
    "@heroui/switch": "2.2.26",
    "@heroui/system": "2.4.25",
    "@heroui/table": "2.2.29",
    "@heroui/theme": "2.4.25",
    "@heroui/tooltip": "2.2.26",
    "@internationalized/date": "3.10.1",
    "@radix-ui/react-toggle-group": "1.1.11",
    "@sentry/nextjs": "10.33.0",
    "@tanstack/react-query": "5.90.16",
    "@vercel/analytics": "1.6.1",
    "@vercel/speed-insights": "1.3.1",
    "@vis.gl/react-google-maps": "1.7.1",
    "city-timezones": "1.3.3",
    "es-toolkit": "1.43.0",
    "lucide-react": "0.562.0",
    "moment-timezone": "0.6.0",
    "motion": "12.26.1",
    "next": "16.1.1",
    "next-intl": "4.7.0",
    "next-mdx-remote": "5.0.0",
    "next-themes": "0.4.6",
    "openapi-fetch": "0.15.0",
    "openapi-react-query": "0.5.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "5.5.0",
    "react-syntax-highlighter": "16.1.0",
    "use-debounce": "10.1.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@lingual/i18n-check": "0.8.17",
    "@msw/playwright": "0.4.5",
    "@next/eslint-plugin-next": "16.1.1",
    "@playwright/test": "1.57.0",
    "@storybook/addon-docs": "10.1.11",
