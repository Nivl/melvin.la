    "openapi-typescript": "7.10.1",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "storybook": "10.1.11",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vitest": "4.0.17",
    "vitest-canvas-mock": "1.1.3"
  },
  "imports": {
    "#hooks/fortnite/useStats": {
      "test": "./src/hooks/fortnite/useStats.mock.ts",
      "default": "./src/hooks/fortnite/useStats.tsx"
    },
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.tsx",
      "./src/*/index.ts"
    ]
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
