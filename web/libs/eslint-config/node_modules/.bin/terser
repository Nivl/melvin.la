#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules/terser/bin/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules/terser/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules/terser/bin/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules/terser/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/terser@5.44.1/node_modules:/Users/melvin/dev/dups/melvin.la/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/terser@5.44.1/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/terser@5.44.1/node_modules/terser/bin/terser" "$@"
fi
